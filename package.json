{
  "name": "dotenv-jsonc",
  "repository": "github:fabiospampinato/dotenv-jsonc",
  "description": "Simple library for loading your .env.json file containing JSONC.",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./register": {
      "default": "./dist/register.js"
    }
  },
  "scripts": {
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "test": "tsex test",
    "test:watch": "tsex test --watch",
    "prepublishOnly": "npm run clean && npm run compile && npm run test"
  },
  "keywords": [
    "dotenv",
    "json",
    "jsonc"
  ],
  "dependencies": {
    "jsonc-simple-parser": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "fava": "^0.0.7",
    "tsex": "^1.1.4",
    "typescript": "^4.9.4"
  }
}
