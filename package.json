    "test:watch": "tsex test --watch",
    "prepublishOnly": "tsex prepare"
  },
  "keywords": [
    "dotenv",
    "json",
    "jsonc"
  ],
  "dependencies": {
    "tiny-jsonc": "^1.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.19.74",
    "fava": "^0.3.4",
    "tsex": "^4.0.2",
    "typescript": "^5.7.3"
  }
}
