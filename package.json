{
  "name": "dotenv-jsonc",
  "repository": "github:fabiospampinato/dotenv-jsonc",
  "description": "Simple library for loading your .env.json file containing JSONC.",
  "license": "MIT",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./register": {
      "default": "./dist/register.js"
    }
  },
  "scripts": {
    "clean": "tsex clean",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
